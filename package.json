{
  "name": "kawaii-image-api",
  "version": "0.1.0",
  "description": "REST API to serve different images or gifs from animes",
  "main": "index.js",
  "scripts": {
    "start": "node build/index.js",
    "predev": "npm run swagger",
    "prebuild": "npm run swagger",
    "build": "tsc",
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec\"",
    "swagger": "tsoa spec",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eme22/kawaii-image-api.git"
  },
  "keywords": [],
  "author": "eme22",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eme22/kawaii-image-api/issues"
  },
  "homepage": "https://github.com/eme22/kawaii-image-api#readme",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.0",
    "@types/jest": "^27.4.1",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.21",
    "@types/passport": "^1.0.7",
    "@types/swagger-ui-express": "^4.1.3",
    "concurrently": "^7.0.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@discordx/importer": "^1.1.2",
    "axios": "^0.27.2",
    "discord.js": "^13.6.0",
    "discordx": "^9.5.3",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "express-session": "^1.17.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "passport": "^0.5.2",
    "passport-discord": "^0.1.4",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.3.0",
    "tsoa": "^3.2.1",
    "typeorm": "^0.2.44"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "ts-node src/index.ts"
  },
  "swagger": {
    "basePath": "adada"
  }
}
